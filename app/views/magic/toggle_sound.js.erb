let volumeUpButton = document.getElementById('player-volume-up');
let volumnMuteButton = document.getElementById('player-volume-mute');

if (volumeUpButton.classList.contains('active')) {
  volumeUpButton.classList.remove('active');
  volumnMuteButton.classList.add('active');
} else {
  volumnMuteButton.classList.remove('active');
  volumeUpButton.classList.add('active');
}